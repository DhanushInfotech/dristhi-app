<form autocomplete="off" class="or clearfix" id="Child_Registration_EngKan" novalidate="novalidate">
    <!--This form was created by transforming a OpenRosa-flavored (X)Form using an XSLT sheet created by Enketo LLC.-->
    <section class="form-logo"></section>

    <h3 id="form-title">Edit Child Registration</h3>
    <select style="display:none;" data-default-lang="default" id="form-languages">
        <option value="default">default</option>
    </select>

    <label class="question non-select ">
        <span class="question-label active" lang="">Edit password:</span>
        <input name="/Child_Registration_EngKan/edit_password" autocomplete="off"
            data-type-xml="string" type="password" />
    </label>
    <section name="/Child_Registration_EngKan/child_edit_group"
        class="or-group-data or-branch pre-init "
        data-relevant=" /Child_Registration_EngKan/edit_password  = /Child_Registration_EngKan/child_edit_group/pwd">

        <!--<label class="question non-select ">-->
        <!--<span class="question-label active" lang="">PHC:-->
        <!--</span>-->
        <!--<input name="/Child_Registration_EngKan/child_edit_group/phc" autocomplete="off"-->
        <!--data-type-xml="string" disabled="disabled" type="text" />-->
        <!--</label>-->

        <!--<label class="question non-select ">-->
        <!--<span class="question-label active" lang="">Sub Center:-->
        <!--</span>-->
        <!--<input name="/Child_Registration_EngKan/child_edit_group/sub_center" autocomplete="off"-->
        <!--data-type-xml="string" disabled="disabled" type="text" />-->
        <!--</label>-->
        <!--<label class="question non-select ">-->
        <!--<span class="question-label active" lang="">Village:-->
        <!--</span>-->
        <!--<input name="/Child_Registration_EngKan/child_edit_group/village" autocomplete="off"-->
        <!--data-type-xml="string" disabled="disabled" type="text" />-->
        <!--</label>-->
        <section name="/Child_Registration_EngKan/child_edit_group/child">
            <fieldset class="question ">
                <fieldset>
                    <legend>
                        <span class="question-label active" lang="">Sex of child?
                        </span>
                        <span class="required">*</span>
                    </legend>
                    <div class="option-wrapper">
                        <label class="">
                            <input name="/Child_Registration_EngKan/child_edit_group/sex_child"
                                autocomplete="off"
                                data-name="/Child_Registration_EngKan/child_edit_group/sex_child"
                                data-type-xml="select1" required="required" type="radio"
                                value="male" />
                            <span class="option-label active" lang="">Male</span>
                        </label>
                        <label class="">
                            <input name="/Child_Registration_EngKan/child_edit_group/sex_child"
                                autocomplete="off"
                                data-name="/Child_Registration_EngKan/child_edit_group/sex_child"
                                data-type-xml="select1" required="required" type="radio"
                                value="female" />
                            <span class="option-label active" lang="">Female</span>
                        </label>
                    </div>
                </fieldset>
            </fieldset>
            <label class="question non-select ">
                <span class="question-label active" lang="">Child birthweight (in kgs)?
                </span>
                <input name="/Child_Registration_EngKan/child_edit_group/birthweight"
                    autocomplete="off" data-type-xml="decimal" step="any" type="number" />
            </label>
            <label class="question or-appearance-minimal ">
                <span class="question-label active" lang="">Child blood group?
                </span>
                <select name="/Child_Registration_EngKan/child_edit_group/child_blood_group"
                    data-name="/Child_Registration_EngKan/child_edit_group/child_blood_group"
                    data-type-xml="select1">
                    <option value="">...</option>
                    <option value="a_positive">A+</option>
                    <option value="a_negative">A-</option>
                    <option value="b_positive">B+</option>
                    <option value="b_negative">B-</option>
                    <option value="ab_positive">AB+</option>
                    <option value="ab_negative">AB-</option>
                    <option value="o_positive">O+</option>
                    <option value="o_negative">O-</option>
                </select>
                <span style="display:none;" class="or-option-translations"></span>
            </label>
            <fieldset class="question ">
                <fieldset>
                    <legend>
                        <span class="question-label active" lang="">Immunizations provided at
                            birth?
                        </span>
                    </legend>
                    <div class="option-wrapper">
                        <label class="">
                            <input
                                name="/Child_Registration_EngKan/child_edit_group/immunizations_atbirth"
                                autocomplete="off" data-type-xml="select" type="checkbox"
                                value="bcg" />
                            <span class="option-label active" lang="">Bacillus
                                Calmetteâ€“Guerin(BCG)
                            </span>
                        </label>
                        <label class="">
                            <input
                                name="/Child_Registration_EngKan/child_edit_group/immunizations_atbirth"
                                autocomplete="off" data-type-xml="select" type="checkbox"
                                value="opv_0" />
                            <span class="option-label active" lang="">Oral Polio Vaccine(OPV) 0
                            </span>
                        </label>
                        <label class="">
                            <input
                                name="/Child_Registration_EngKan/child_edit_group/immunizations_atbirth"
                                autocomplete="off" data-type-xml="select" type="checkbox"
                                value="hepb_0" />
                            <span class="option-label active" lang="">Hep B 0
                            </span>
                        </label>
                    </div>
                </fieldset>
            </fieldset>
            <fieldset class="question or-branch pre-init ">
                <fieldset>
                    <legend>
                        <span class="question-label active" lang="">Hep B prophylaxis provided?
                        </span>
                    </legend>
                    <div class="option-wrapper">
                        <label class="">
                            <input
                                name="/Child_Registration_EngKan/child_edit_group/hepb_prophylaxis_provided"
                                autocomplete="off"
                                data-name="/Child_Registration_EngKan/child_edit_group/hepb_prophylaxis_provided"
                                data-relevant=" /Child_Registration_EngKan/child_edit_group/case_hepb  != ''"
                                data-type-xml="select1" type="radio" value="yes" />
                            <span class="option-label active" lang="">Yes</span>
                        </label>
                        <label class="">
                            <input
                                name="/Child_Registration_EngKan/child_edit_group/hepb_prophylaxis_provided"
                                autocomplete="off"
                                data-name="/Child_Registration_EngKan/child_edit_group/hepb_prophylaxis_provided"
                                data-relevant=" /Child_Registration_EngKan/child_edit_group/case_hepb  != ''"
                                data-type-xml="select1" type="radio" value="no" />
                            <span class="option-label active" lang="">No</span>
                        </label>
                    </div>
                </fieldset>
            </fieldset>
            <label class="note or-branch pre-init non-select ">
                <span class="question-label active" lang="">Baby is premature! Advise kangaroo
                    care and rooming in.
                </span>
                <input name="/Child_Registration_EngKan/child_edit_group/premature_message1"
                    autocomplete="off"
                    data-relevant=" /Child_Registration_EngKan/child_edit_group/premature  != ''"
                    data-type-xml="string" readonly="readonly" type="text" />
            </label>
            <label class="note or-branch pre-init non-select ">
                <span class="question-label active" lang="">Child is at risk because of,
                    <span class="or-output"
                        data-value=" /Child_Registration_EngKan/child_edit_group/child_high_risk_reasons "></span>
                </span>
                <input name="/Child_Registration_EngKan/child_edit_group/high_risk_note_child"
                    autocomplete="off"
                    data-relevant=" /Child_Registration_EngKan/child_edit_group/is_child_high_risk1  = 'yes'"
                    data-type-xml="string" readonly="readonly" type="text" />
            </label>
            <fieldset class="question ">
                <fieldset>
                    <legend>
                        <span class="question-label active" lang="">Do you want to mark child as
                            HR (High Risk)?
                        </span>
                        <span class="required">*</span>
                    </legend>
                    <div class="option-wrapper">
                        <label class="">
                            <input
                                name="/Child_Registration_EngKan/child_edit_group/is_child_high_risk"
                                autocomplete="off"
                                data-name="/Child_Registration_EngKan/child_edit_group/is_child_high_risk"
                                data-type-xml="select1" required="required" type="radio"
                                value="yes" />
                            <span class="option-label active" lang="">Yes</span>
                        </label>
                        <label class="">
                            <input
                                name="/Child_Registration_EngKan/child_edit_group/is_child_high_risk"
                                autocomplete="off"
                                data-name="/Child_Registration_EngKan/child_edit_group/is_child_high_risk"
                                data-type-xml="select1" required="required" type="radio"
                                value="no" />
                            <span class="option-label active" lang="">No</span>
                        </label>
                    </div>
                </fieldset>
            </fieldset>
            <label class="note or-branch pre-init non-select ">
                <span class="question-label active" lang="">Child needs 3 more PNC visits on
                    days 14, 21, and 28
                    because of, 14, 21
                    <span class="or-output"
                        data-value=" /Child_Registration_EngKan/child_edit_group/child_high_risk_reasons "></span>
                </span>
                <input name="/Child_Registration_EngKan/child_edit_group/addl_pnc_visit_message"
                    autocomplete="off"
                    data-relevant=" /Child_Registration_EngKan/child_edit_group/is_child_high_risk1  = 'yes'"
                    data-type-xml="string" readonly="readonly" type="text" />
            </label>
        </section>

    </section>
    <!--end of group -->

    <fieldset style="display:none;" id="or-preload-items">
        <label class="calculation non-select ">
            <input name="/Child_Registration_EngKan/today" autocomplete="off" data-preload="date"
                data-preload-params="today" data-type-xml="date" type="hidden" />
        </label>
    </fieldset>
    <fieldset style="display:none;" id="or-calculated-items">
        <label class="calculation non-select ">
            <input name="/Child_Registration_EngKan/meta/instanceID" autocomplete="off"
                data-calculate="concat('uuid:', uuid())" data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/Child_Registration_EngKan/formhub/uuid" autocomplete="off"
                data-calculate="'52e167b3ca8f4ab4b54b183c6449c1d8'" data-type-xml="string"
                type="hidden" />
        </label>
    </fieldset>
</form>